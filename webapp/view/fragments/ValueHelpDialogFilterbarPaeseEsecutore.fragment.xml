<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">

    <!-- Dialog senza handler, key e desc -->
    <ValueHelpDialog title="Paese Esecutore">

        <filterBar>
            <fb:FilterBar advancedMode="true" isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>

                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Country"
                        label="Country"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Country"/>
                        </fb:control>
                    </fb:FilterGroupItem>

                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Country_Text"
                        label="Descrizione"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Country_Text"/>
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>

    </ValueHelpDialog>
</core:FragmentDefinition>
